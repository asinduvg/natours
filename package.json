{
  "name": "natours-landing",
  "version": "1.0.0",
  "description": "Landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "@asinduvg/natours-landing:node-sass sass/main.scss css/style.css -w",
    "dev-server": "@asinduvg/natours-landing:live-server",
    "start": "@asinduvg/natours-landing:npm-run-all --parallel dev-server watch:sass",
    "compile:sass": "@asinduvg/natours-landing:node-sass sass/main.scss css/style.comp.css",
    "concat:css": "@asinduvg/natours-landing:concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "@asinduvg/natours-landing:postcss --use @asinduvg/natours-landing:autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "@asinduvg/natours-landing:node-sass css/style.prefix.css css/style.min.css --output-style compressed",
    "build:css": "@asinduvg/natours-landing:npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/asinduvg/natours-landing.git"
  },
  "author": "Asindu",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/asinduvg/natours-landing/issues"
  },
  "homepage": "https://gitlab.com/asinduvg/natours-landing#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^4.1.1"
  }
}
